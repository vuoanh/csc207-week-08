package edu.grinnell.sortingvisualizer.sortevents;
import java.util.List;
public class SortEvent {
	public static <T extends Comparable<? super T>> void apply(T arr[]){
		arr[].apply();
	}
	public <Integer> List<> getAffectedIndices(){
		return new ArrayList<Integer>;
	}
	public static boolean isEmphasized(){
		if(emphasized){
			return true;
		}
		else{
			return false;
		}
	}
}
