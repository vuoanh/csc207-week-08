
public class LinkedList  <T> implements List <T>{
	private Node first;
	private int size;
	
	// Hide the Node class
	private class Node<T>{
		public T value;
		public Node next;
		// Constructor
		public Node(T value, Node<T> next){
			this.value = value;
			this.next = next;
		}
		public Node(T value){
			this.(value, null);
		}
	}
	
	public LinkedList(){
		first = null;
		size = 0;
	}

	@Override
	public void add(T variable) {
		Node<T> newNode = new Node<T>(variable, first);
	}

	@Override
	public void remove(T variable) {
